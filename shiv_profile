export PS1="\n\[\033[0;94m\]\t:\w/ >> \[\033[0m\]"


alias spacer="defaults write com.apple.dock persistent-apps -array-add '{tile-data={}; tile-type="spacer-tile";}';killall Dock;"
alias qfinder="defaults write com.apple.finder QuitMenuItem -bool yes; killall Finder;"
alias showhidden="defaults write com.apple.finder AppleShowAllFiles YES; killall Finder"

alias ga='git add .'
alias gs='git status'
alias gst='gs'
alias gu='git pull'
alias gpl='git pull'
alias gpull='git pull'
alias gps='git push'
alias gph='git push'
alias gpush='git push'
alias gdf='git diff'
alias gd='git diff'
alias gc='git commit $1' 
alias gc='git commit -a -m '
alias gb='git branch'
alias gba='git branch -a'
alias gco='git checkout'
alias gcom='git checkout master'
alias gcob='git checkout -b'
alias gcot='git checkout -t'
alias gcotb='git checkout --track -b'
alias glog='git log'
alias gl='git log -3'
alias gr='git reset --hard head'
alias gsa='git stash apply'
alias gsl='git stash list'
alias gsp='git stash pop'

function gacp() {
    git add .
    git commit -a -m "$1"
    git push
}

function gcp() {
    git cherry-pick $1
}

function gc() {
    git checkout $1
}

alias vip='vim ~/.profile'
alias visp='vim ~/code/shiv-util/shiv_profile'
alias sp='source ~/.profile'
alias cc='cd ~/code/'
alias cb='cd ~/code/b*'
alias cdb='cb'
alias cdsu='cd ~/code/shiv-util'
alias cdsp='cd ~/code/shiv-docs/'

function pyhton() {
    python "$@"
}
